{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 8920,
      "guest_os_type": "RedHat_64",
      "headless": false,
      "http_directory": "kickstart",
      "iso_urls": [
        "../os/CentOS-6.7-x86_64-bin-DVD1.iso",
        "http://ftp.hosteurope.de/mirror/centos.org/6.7/isos/x86_64/CentOS-6.7-x86_64-bin-DVD1.iso"
      ],
      "iso_checksum_type": "md5",
      "iso_checksum": "51f90e8ddd2ea199780065eaf75130ad",
      "ssh_username": "root",
      "ssh_password": "#centos!",
      "ssh_port": 10022,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "/sbin/halt -h -p",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "virtualbox_version_file": ".vbox_version",
      "vm_name": "packer-centos-6.7-x86_64",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "1024"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--nic1",
          "nat"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--natpf1",
          "guestssh,tcp,127.0.0.1,10022,,22"
        ]
      ]
    }
  ]
}

