{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter><wait>"
      ],
      "boot_wait": "20s",
      "disk_size": 8920,
      "guest_os_type": "RedHat_64",
      "headless": true,
      "http_directory": "kickstart",
      "iso_urls": [
        "/data/packages/centos/CentOS-6.7-x86_64-LiveDVD.iso",
        "http://centos.crazyfrogs.org/6.7/isos/x86_64/CentOS-6.7-x86_64-LiveDVD.iso"
      ],
      "iso_checksum_type": "md5",
      "iso_checksum": "10745b3aa580742fd1cc72488eca0ebd",
      "ssh_username": "root",
      "ssh_password": "centos",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/halt -h -p",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "virtualbox_version_file": ".vbox_version",
      "vm_name": "packer-centos-6.7-x86_64",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "1024"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--longmode",
          "off"
        ]
      ]
    }
  ]
}

